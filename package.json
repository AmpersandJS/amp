{
  "name": "amp",
  "description": "A collection of utility modules as independent modules.",
  "version": "1.0.0",
  "author": "Henrik Joreteg <henrik@andyet.net>",
  "bugs": "https://github.com/ampersandjs/amp/issues",
  "dependencies": {
    "browserify": "^6.3.3",
    "columnizer": "",
    "fixpack": "^1.2.0",
    "fs-extra": "^0.12.0",
    "http-server": "^0.7.3",
    "jade": "^1.7.0",
    "marked": "^0.3.2",
    "phantomjs": "^1.9.12",
    "precommit-hook": "^1.0.7",
    "rimraf": "^2.2.8",
    "tap-spec": "^2.1.0",
    "tape": "^3.0.3",
    "tape-run": "^0.3.0",
    "zuul": "^1.13.1"
  },
  "homepage": "http://amp.ampersandjs.com",
  "keywords": [
    "util",
    "collection",
    "amp"
  ],
  "license": "MIT",
  "packages": {
    "arrays": [
      "index-of",
      "last"
    ],
    "collections": [
      "contains",
      "each",
      "size"
    ],
    "dom": [],
    "functions": [
      "before",
      "bind",
      "debounce",
      "delay",
      "once"
    ],
    "objects": [
      "clone",
      "defaults",
      "extend",
      "has",
      "invert",
      "keys",
      "values"
    ],
    "strings": [
      "escape",
      "to-camel-case"
    ],
    "utils": [
      "is-arguments",
      "is-array",
      "is-boolean",
      "is-date",
      "is-empty",
      "is-function",
      "is-nan",
      "is-null",
      "is-number",
      "is-object",
      "is-object-equal",
      "is-regexp",
      "is-string",
      "is-undefined",
      "result",
      "unique-id"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ampersandjs/amp"
  },
  "scripts": {
    "before-test": "node bin/gen-testrunner",
    "build": "npm run clean && npm run folders && npm run write-package-files && npm run readmes && npm run before-test && npm run build-site",
    "build-site": "node bin/build-site",
    "clean": "node bin/sort-modules && fixpack",
    "folders": "node bin/gen-folders",
    "make-local": "node bin/convert-requires local",
    "make-remote": "node bin/convert-requires remote",
    "publish": "node bin/publish-all",
    "publish-site": "git checkout gh-pages && git merge master && git push origin gh-pages && git checkout master",
    "readmes": "node bin/build-readmes",
    "show-tree": "node bin/show-tree",
    "start": "npm run build-site && http-server -p 8000",
    "test": "browserify bin/test-all.js | tape-run | tap-spec",
    "test-node": "node bin/test-server.js | tap-spec",
    "test-browser": "zuul --local 8080 -- bin/test-all.js",
    "test-ci": "zuul -- bin/test-all.js",
    "validate": "node bin/check-doc.js",
    "write-package-files": "node bin/write-package-files"
  }
}
